    "rxjs": "^7.8.1",
    "virtua": "^0.30.1"
  },
  "devDependencies": {
    "@biomejs/biome": "^1.6.3",
    "@pandacss/dev": "^0.37.2",
    "@shadow-panda/preset": "^0.7.1",
