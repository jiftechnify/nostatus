    "@radix-ui/react-select": "^2.0.0",
    "@radix-ui/react-tabs": "^1.0.4",
    "@shadow-panda/style-context": "^0.7.1",
    "i18next": "^23.8.2",
    "i18next-browser-languagedetector": "^7.2.1",
    "jotai": "^2.7.2",
    "js-easing-functions": "^1.0.3",
